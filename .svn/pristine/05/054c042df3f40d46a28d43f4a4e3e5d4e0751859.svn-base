
package containers;

import java.util.ArrayList;



public class CArray extends ACollection {

	private ArrayList<Object> v = new ArrayList<Object>();

	/**
	 * @param size
	 */
	public CArray(int size) {
		v= new ArrayList<Object>(size);
	}

	/**
	 * @param idx
	 * @return
	 */
	public Object getAt(int idx) {
		return v.get(idx);
	}

	/**
	 * @param idx
	 * @param data
	 */
	public void setAt(Object data, int idx) {
		v.set(idx, data);
	}
}

